CREATE SCHEMA `home_pharmacy` ;

CREATE TABLE `home_pharmacy`.`home` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `family_name` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`id`));

CREATE TABLE `home_pharmacy`.`family_member` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `name` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`id`));

CREATE TABLE `home_pharmacy`.`medicine` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `name` VARCHAR(45) NOT NULL,
  `type` VARCHAR(45) NOT NULL,
  `contents` DOUBLE NOT NULL,
  `term_of_validity` DATE NOT NULL,
  `allowed_duration_of_use` DATE NOT NULL,
  PRIMARY KEY (`id`));




